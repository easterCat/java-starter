{
  "title": "Java-starter",
  "author": "qingcheng",
  "language": "zh-hans",
  "plugins": [
    "theme-comscore",
    "toggle-chapters",
    "expandable-chapters",
    "chapter-fold",
    "edit-link",
    "lightbox",
    "copy-code-button",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "edit-link": {
      "base": "//github.com/easterCat/java-starter/master",
      "label": "编辑此页面"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": true,
      "isShowTocTitleIcon": true,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
